{
    "name": "gridfs_stream_local",
    "version": "0.2.0",
    "description": "Local, mostly drop-in replacement to gridfs",
    "main": "Grid.js",
    "repository": "https://github.com/Cloud-V/gridfs_stream_local",
    "author": "Omar Diaa <omardiaa500@gmail.com>",
    "license": "Apache-2.0",
    "private": false,
    "dependencies": {
        "@actions/core": "^1.9.0",
        "fs-extra": "^10.1.0",
        "mongoose": "^6.1.10"
    },
    "scripts": {
        "format": "node ./node_modules/prettier/bin-prettier.js --tab-width 4 --write .",
        "lint": "node ./node_modules/prettier/bin-prettier.js --tab-width 4 --write .",
        "mongo": "node -e \"require('fs-extra').ensureDir('./mongo')\" && mongod --dbpath ./mongo",
